MainSection:NewToggle("Auto Meditate", "Auto Meditate", function(Meditate)
getgenv().Meditate = Meditate

local LP = game:GetService("Players").LocalPlayer
local VIM = game:GetService("VirtualInputManager")
local key

if getgenv().Meditate then
    LP.Character.HumanoidRootPart.Anchored = true
else
    LP.Character.HumanoidRootPart.Anchored = false
end
while getgenv().Meditate do
    task.wait()
    if LP.Stamina.Value <= 30 then
        repeat
            task.wait()
        until LP.PlayerGui.MainGui.Stamina.AbsoluteSize == LP.PlayerGui.MainGui.Stamina.Clipping.AbsoluteSize
    end
    for i,v in pairs(LP.Backpack:GetChildren()) do
        if v.Name == "Meditate" then
            v.Parent = LP.Character
        end
    end
    for y, z in pairs(LP.PlayerGui.MainGui.Keys:GetChildren()) do
        if z.Visible == true then
            key = z.Name
        end
    end
    for y, z in pairs(LP.Character:GetChildren()) do
        if z:IsA("Tool") and z.Name == "Meditate" then
            if LP.Stamina.Value < 30 then
             else
                if key == "z" then
                    VIM:SendKeyEvent(true, Enum.KeyCode.Z, false, game)
                elseif key == "r" then
                    VIM:SendKeyEvent(true, Enum.KeyCode.R, false, game)
                elseif key == "e" then
                    VIM:SendKeyEvent(true, Enum.KeyCode.E, false, game)
                elseif key == "w" then
                    VIM:SendKeyEvent(true, Enum.KeyCode.W, false, game)
                elseif key == "q" then
                    VIM:SendKeyEvent(true, Enum.KeyCode.Q, false, game)
                end
            end
        end
    end
end
end)
