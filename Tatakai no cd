MainSection:NewToggle("Auto Pullup", "Auto Pullup", function(Pullup)
    getgenv().Pullup = Pullup

local serv = game:GetService("PathfindingService")
local human = game.Players.LocalPlayer.Character.Humanoid
local body = game.Players.LocalPlayer.Character.Torso or game.Players.LocalPlayer.Character.HumanoidRootPart
local function Walk(x, y, z)
local dest = Vector3.new(x, y, z)
local path = serv:CreatePath()
path:ComputeAsync(body.Position,dest)
if path.Status == Enum.PathStatus.Success then
   local wayPoints = path:GetWaypoints()
   for i = 1,#wayPoints do
       local point = wayPoints[i]
       human:MoveTo(point.Position)
       local success = human.MoveToFinished:Wait()
       if point.Action == Enum.PathWaypointAction.Jump then
           human.WalkSpeed = 0
           wait(0.2)
           human.WalkSpeed = 16
           human.Jump = true
       end
       if not success then
           print("trying again")
           human.Jump = true
           human:MoveTo(point.Position)
           if not human.MoveToFinished:Wait() then
               break
           end
       end
   end
end
end
while getgenv().Pullup do task.wait(0.1)
if not game.Players.LocalPlayer.Character:FindFirstChild("Pullup") then
local dist = {}
for i,v in next, game:GetService("Workspace").TrainingStations:GetDescendants() do
    if v.ClassName == "ClickDetector" then
        local magn = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Parent.Main.Position).Magnitude
        table.insert(dist, magn)
    end
end

for i,v in next, game:GetService("Workspace").TrainingStations:GetDescendants() do
    if v.ClassName == "ClickDetector" then
        local magn2 = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Parent.Main.Position).Magnitude
        if magn2 == math.min(unpack(dist)) then
            Walk(v.Parent.Main.Position.X, game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Y, v.Parent.Main.Position.Z)
            fireclickdetector(v)
        end
    end
end
else
    game:GetService("VirtualInputManager"):SendMouseButtonEvent(0, 0, 0, true, game, 0)
end
end
end)
MainSection:NewToggle("Auto Ramen", "Auto Ramen", function(Ramen)
    getgenv().Ramen = Ramen

    while getgenv().Ramen do
        wait(5)
        local size = game:GetService("Players").LocalPlayer.PlayerGui.MainGui.Hunger.Clipping.AbsoluteSize
        local x = size.X
        if x < 25 then
            for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                if v.Name == "Ramen" then
                    if not game.Players.LocalPlayer.Character:FindFirstChild("Ramen") then
                        v.Parent = game.Players.LocalPlayer.Character
                        game.Players.LocalPlayer.Character:FindFirstChild("Ramen"):Activate()
                        v.Parent = game.Players.LocalPlayer.Backpack
                    end
                end
            end
        end
    end
end)
