function respawn(plr)
	local char = plr.Character
	if char:FindFirstChildOfClass("Humanoid") then char:FindFirstChildOfClass("Humanoid"):ChangeState(15) end
	char:ClearAllChildren()
	local newChar = Instance.new("Model")
	newChar.Parent = workspace
	plr.Character = newChar
	wait()
	plr.Character = char
	newChar:Destroy()
end

respawn(game.Players.LocalPlayer)
game.Players.LocalPlayer.CharacterAdded:Wait()
local part = Instance.new("Part")
part.Anchored = true
part.Position = Vector3.new(-573500.17502689, -0.99106359, -57350.165924)
part.Size = Vector3.new(200,20,200)
part.Parent = game.Workspace
part.Name = "Amaterasu on top"
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = CFrame.new(game.Workspace["Amaterasu on top"].Position + Vector3.new(0,20,0))
end)
